    device = "/dev/disk/by-uuid/629538ab-a34c-441d-a379-1cca6d799075";
    fsType = "ext4";
  };

  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/52D0-5E93";
    fsType = "vfat";
    options = ["fmask=0022" "dmask=0022"];
  };

  swapDevices = [
    {device = "/dev/disk/by-uuid/aaefd6d2-58e3-4b3a-b229-14f56d772ff8";}
